page {
    background: #f3f5f8;
}

.top-search-box {
    background: #fff;
    box-sizing: border-box;
    padding: 32rpx;
}

.top-search {
    background: #f6f7f9;
    border-radius: 60rpx;
    box-sizing: border-box;
    display: flex;
    height: 88rpx;
    padding: 23rpx 40rpx 25rpx;
}

.top-search .img-search {
    height: 36rpx;
    margin-right: 17rpx;
    margin-top: 3rpx;
    width: 36rpx;
}

.top-search .search-text {
    flex: 1;
    line-height: 40rpx;
}

.result-num,.top-search .search-text {
    color: #999;
    font-family: PingFangSC-Regular,PingFang SC;
    font-size: 28rpx;
    font-weight: 400;
}

.result-num {
    height: 104rpx;
    line-height: 104rpx;
    text-align: center;
}

.score {
    color: #666;
    display: flex;
    flex-wrap: wrap;
    font-family: PingFangSC-Regular,PingFang SC;
    font-size: 28rpx;
    font-weight: 400;
    line-height: 40rpx;
}

.score view {
    margin-right: 30rpx;
    margin-top: 24rpx;
}

.score view:nth-child(3n) {
    text-align: right;
}

.score view:nth-child(3n-1) {
    box-sizing: border-box;
    padding-left: 20rpx;
}

.help_item .title {
    background: #fff;
    border-bottom: 1rpx solid #e7eef0;
    box-sizing: border-box;
    color: #333;
    display: flex;
    font-family: PingFangSC-Medium,PingFang SC;
    font-size: 32rpx;
    height: 112rpx;
    justify-content: space-between;
    line-height: 112rpx;
    padding: 0 32rpx;
}

.help_item .title .title_1 {
    font-weight: 600;
}

.plan-item,.plan-list {
    background-color: #fff;
}

.plan-item {
    align-items: center;
    border-bottom: 1rpx solid #e7eef0;
    display: flex;
    height: 113rpx;
    justify-content: space-between;
    line-height: 113rpx;
    margin: 0 32rpx;
}

.plan-item .plan-item-text {
    color: #333;
    font-size: 32rpx;
    font-weight: 600;
}

.plan-item .top-icon {
    height: 36rpx;
    vertical-align: middle;
    width: 36rpx;
}

.plan-item-detail {
    background-color: #f3f5f8;
    box-sizing: border-box;
    display: flex;
    flex-wrap: wrap;
    line-height: 114rpx;
    padding: 0 32rpx;
}

.batch-num,.plan-num {
    color: #666;
    width: 320rpx;
}

.plan-num {
    margin-left: 46rpx;
}