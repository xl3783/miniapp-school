<wxs module="tool">
var getSort = (function(dataInfo, id) {
    var arr = dataInfo.questions.filter((function(item) {
        return (item.is_show == 1)
    }));
    var index = 0;
    arr.forEach((function(item, i) {
        if (id == item.id) {
            index = i
        }
    }));
    return (index + 1)
});
var getFileTypes = (function(types) {
    if (types) {
        if (types[(0)] != 0) {
            var arr = ["word", "xls", "xlsx", "csv", "ppt", "pdf", "zip", "rar"];
            var str = [];
            types.forEach((function(item) {
                str.push(arr[((nt_1 = (item - 1), null == nt_1 ? undefined : 'number' === typeof nt_1 ? nt_1 : "" + nt_1))])
            }));
            return (str.join(","))
        }
    };
    return ("xls、word")
});
module.exports.getSort = getSort;
module.exports.getFileTypes = getFileTypes;
</wxs>